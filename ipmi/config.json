{
  "name": "IPMI",
  "version": "1.8.18",
  "slug": "ipmi",
  "description": "Expose IPMI sensor to Home Assistant",
  "url": "https://github.com/evilmarty/hassio-addons/tree/master/ipmi",
  "arch": ["amd64"],
  "startup": "services",
  "init": false,
  "boot": "auto",
  "services": ["mqtt:need"],
  "devices": [
    "/dev/ipmi0"
  ],
  "options": {
    "host": localhost,
    "user": null ,
    "password": null,
    "interval": 5,
    "discovery_prefix": "homeassistant",
    "debug": false
  },
  "schema": {
    "host": "str",
    "user": "str" ,
    "password": "str",
    "interval": "int(1,)",
    "discovery_prefix": "str",
    "debug": "bool"
  }
}
